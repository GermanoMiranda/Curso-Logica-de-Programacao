programa {
  funcao inicio() {
    inteiro numero1
    inteiro numero2
    inteiro soma
    
    escreva("Digite um número inteiro positivo:")
    leia(numero1)
    escreva("Digite outro número inteiro positivo:")
    leia(numero2)

    soma = numero1 + numero2

    escreva("A soma de ",numero1, " + ", numero2, " = ", soma)


  }
}
