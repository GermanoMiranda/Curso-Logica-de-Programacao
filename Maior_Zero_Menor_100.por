programa {
  funcao inicio() {
    inteiro numero
    escreva("Digite um número inteiro positivo: ")
    leia(numero)

       se(numero > 0 e numero < 100)

       {
        escreva(numero," é maior que 0 e menor que 100.")
       }
        senao
        escreva(numero," Tente novamente")


       }

  }
}
