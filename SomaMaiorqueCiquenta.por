programa {
  funcao inicio() {
    inteiro numero1, numero2, soma
  
    escreva("Digite um número inteiro positivo:")
    leia(numero1)
   escreva("Digite um número inteiro positivo:")
   leia(numero2)
   soma = numero1 + numero2

   se( soma > 50)
   {
   escreva("A soma ",numero1," + ",numero2, " = ", soma,"  é maior que 50")
  }
  senao
   escreva("A soma ",numero1," + ",numero2 ," = " ,soma,"  é menor que 50")
  }
}




