programa {
  funcao inicio() {
    inteiro numero
      escreva("digite um númeiro inteiro positivo: ")
      leia(numero)
     se (numero > 100)
    {
       escreva(numero," é maior que 100.")
       }
       senao  se (numero == 100)
        {
          escreva(numero, " é igual a 100.")
          }
    senao 
       escreva (numero," é menor que 100.")
  }
}
