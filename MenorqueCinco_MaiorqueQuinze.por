programa {
  funcao inicio() {
    inteiro numero
    escreva("Digite um número inteiro: ")
    leia(numero)

    se(numero < 5 )

    escreva(numero,"é menor que 5 ")
    senao
    se(numero > 15)
    escreva(numero,"  maior que 15")
    senao
    escreva(" O número é maior que 5 e menor que 15.")
  }
}
