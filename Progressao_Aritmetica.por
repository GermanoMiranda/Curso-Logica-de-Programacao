programa {
  funcao inicio() {
    inteiro numero1, razao, numero2
    escreva("Progressão Aritmética\n")
    escreva("Digite um número inteiro: ")
    leia(numero1)
    escreva("Digite a razão: ")
    leia(razao)

    numero2 = numero1 + razao
    
    escreva("O primero termo é", numero1, " O próximo termo é ", numero2)
  }
}
